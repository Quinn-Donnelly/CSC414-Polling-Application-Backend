{
  "name": "question",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "title": {
      "type": "string",
      "required": true
    },
    "text": {
      "type": "string",
      "required": true
    },
    "type": {
      "type": "object",
      "required": true
    },
    "active": {
      "type": "boolean",
      "required": true,
      "default": true
    }
  },
  "validations": [],
  "relations": {
    "classroom": {
      "type": "belongsTo",
      "model": "classroom",
      "foreignKey": "classroomId"
    },
    "answers": {
      "type": "hasMany",
      "model": "answer",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    }
  ],
  "methods": {}
}
