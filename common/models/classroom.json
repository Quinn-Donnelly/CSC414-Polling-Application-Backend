  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "active": {
      "type": "boolean",
      "required": true,
      "default": true
    },
    "secure": {
      "type": "boolean",
      "required": true,
      "default": false
    }
  },
  "validations": [],
  "relations": {
    "questions": {
      "type": "hasMany",
      "model": "question",
      "foreignKey": ""
    },
    "students": {
      "type": "hasMany",
      "model": "student",
      "foreignKey": ""
    }
  },
  "acls": [],
  "methods": {}
}
